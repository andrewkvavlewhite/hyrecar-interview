{
  "name": "express-calendar",
  "version": "1.0.0",
  "scripts": {
    "postinstall": "cd server && npm install && cd ../client && npm install",
    "client": "cd client && npm start",
    "server": "cd server && npm start",
    "start": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\""
  },
  "dependencies": {},
  "devDependencies": {
    "concurrently": "^4.0.1"
  }
}
